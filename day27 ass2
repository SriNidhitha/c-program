#include<stdio.h>
main()
{
	int n,i,max,min;
	scanf("%d",&n);
	int a[n];//
	for(i=0;i<n;i++)
	scanf("%d",&a[i]);
	max=a[0];
	for(i=0;i<n;i++)
	{                                                      
	if(a[i]>max)
	max=a[i];
}
	printf("max=%d",max);
{

{
		min=a[0];
		for(i=0;i<n;i++)
	if(a[i]<min)
	min=a[i];
}
      	printf("min=%d",min);	
}
}
